<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 边框的颜色及形状 -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#EEB1AC" />
            <corners android:radius="5dp" />
        </shape>
    </item>
    <!--  背景的颜色及形状  -->
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle">
            <solid android:color="#EEB1AC" />
            <corners android:radius="5dp" />
        </shape>
    </item>


</layer-list>
